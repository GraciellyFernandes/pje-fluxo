<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="(REDIST) Fluxo básico geral para redistribuição por extinção">
    <description><![CDATA[........]]></description>  
    <!-- SWIMLANES -->
    <swimlane name="Nó de Desvio - (REDIST) Fluxo básico geral para redistribuição por extinção">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('-1:1338,1342:1469')}"/>
    </swimlane>
    <swimlane name="Secretaria">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('5:1338')}" actor-id="#{actor.id}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial" swimlane="Secretaria"/>
        <transition to="(REDIST) É Fazenda ?" name="(REDIST) É Fazenda ?"/>
    </start-state>  
    <!-- NODES -->
    <end-state name="Término">
        <event type="node-enter">
            <action expression="#{tramitacaoProcessualService.apagaVariavel('pje:tjrn:redistExt')}"/>
        </event>
    </end-state>
    <task-node end-tasks="true" name="Nó de Desvio - (REDIST) Fluxo básico geral para redistribuição por extinção">
        <task name="Nó de Desvio - (REDIST) Fluxo básico geral para redistribuição por extinção" swimlane="Nó de Desvio - (REDIST) Fluxo básico geral para redistribuição por extinção"/>
        <transition to="Término" name="Término"/>
        <transition to="(REDIST) Processos redistribuidos por extinção de órgão" name="(REDIST) Processos redistribuidos por extinção de órgão"/>
    </task-node>
    <decision expression="#{processoTrfHome.possuiCompetencia(10) ? '(REDIST) Redistribuição Juizados Fazenda' : '(REDIST) É Família ?'}" name="(REDIST) É Fazenda ?">
        <transition to="(REDIST) Redistribuição Juizados Fazenda" name="(REDIST) Redistribuição Juizados Fazenda"/>
        <transition to="(REDIST) É Família ?" name="(REDIST) É Família ?"/>
    </decision>
    <decision expression="#{processoTrfHome.possuiCompetencia(15) ? '(REDIST) Reditribuição Varas Família' : 'Término'}" name="(REDIST) É Família ?">
        <transition to="(REDIST) Reditribuição Varas Família" name="(REDIST) Reditribuição Varas Família"/>
        <transition to="Término" name="Término"/>
    </decision>
    <process-state name="(REDIST) Redistribuição Juizados Fazenda">
        <sub-process name="(REDIST) Fluxo para redistribuição por extinção de juizados especiais da Fazenda" binding="late"/>
        <transition to="(REDIST) Foi redistribuído?" name="(REDIST) Foi redistribuído?"/>
    </process-state>
    <process-state name="(REDIST) Reditribuição Varas Família">
        <sub-process name="(REDIST) Fluxo para redistribuição por extinção de varas de Família" binding="late"/>
        <transition to="(REDIST) Foi redistribuído?" name="(REDIST) Foi redistribuído?"/>
    </process-state>
    <decision expression="#{tramitacaoProcessualService.recuperaVariavel('pje:tjrn:redistExt') ? '(REDIST) Processos redistribuidos por extinção de órgão' : 'Término'}" name="(REDIST) Foi redistribuído?">
        <transition to="(REDIST) Processos redistribuidos por extinção de órgão" name="(REDIST) Processos redistribuidos por extinção de órgão"/>
        <transition to="Término" name="Término"/>
    </decision>
    <task-node end-tasks="true" name="(REDIST) Processos redistribuidos por extinção de órgão">
        <task name="(REDIST) Processos redistribuidos por extinção de órgão" swimlane="Secretaria"/>
        <transition to="Nó de Desvio - (REDIST) Fluxo básico geral para redistribuição por extinção" name="Nó de Desvio - (REDIST) Fluxo básico geral para redistribuição por extinção">
            <condition expression="#{true}"/>
        </transition>
        <transition to="Término" name="Cancelar e sair da tarefa"/>
    </task-node>  
    <!-- PROCESS-EVENTS -->
    <event type="node-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="superstate-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="node-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="before-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="superstate-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="transition">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-created">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="after-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-assign">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-create">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="timer">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event> 
</process-definition>
