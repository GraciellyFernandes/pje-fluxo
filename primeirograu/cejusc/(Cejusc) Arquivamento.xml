<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="(Cejusc) Arquivamento">
    <description><![CDATA[.]]></description>  
    <!-- SWIMLANES -->
    <swimlane name="Secretária">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('5:1338,6:1469')}" actor-id="#{actor.id}"/>
    </swimlane>
    <swimlane name="Nó de Desvio - (Cejusc) Arquivamento">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('-1:1338,1342:1469')}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial" swimlane="Secretária"/>
        <transition to="(Cejusc) Lançar movimento de Arquivo Definitivo" name="(Cejusc) Lançar movimento de Arquivo Definitivo"/>
    </start-state>  
    <!-- NODES -->
    <node name="(Cejusc) Lançar movimento de Arquivo Definitivo">
        <transition to="(Cejusc) Arquivo definitivo" name="(Cejusc) Arquivo definitivo"/>
        <event type="node-enter">
            <action expression="#{preencherMovimento.deCodigo(246).lancarMovimento()}"/>
        </event>
    </node>
    <task-node end-tasks="true" name="(Cejusc) Arquivo definitivo">
        <task name="(Cejusc) Arquivo definitivo" swimlane="Secretária"/>
        <transition to="Nó de Desvio - (Cejusc) Arquivamento" name="Nó de Desvio - (Cejusc) Arquivamento">
            <condition expression="#{true}"/>
        </transition>
        <transition to="(Cejusc) Redistribuir" name="Redistribuir processo"/>
    </task-node>
    <task-node end-tasks="true" name="(Cejusc) Redistribuir">
        <task name="(Cejusc) Distribuir" swimlane="Secretária">
            <controller>
                <variable name="Processo_Fluxo_abaRedistribuicaoProcesso" mapped-name="frame:Processo_Fluxo_abaRedistribuicaoProcesso" access="read,write"/>
            </controller>
        </task>
        <transition to="Nó de Desvio - (Cejusc) Arquivamento" name="Nó de Desvio - (Cejusc) Arquivamento">
            <condition expression="#{true}"/>
        </transition>
        <transition to="(Cejusc) Resdistribuido" name="(Cejusc) Resdistribuido">
            <condition expression="#{true}"/>
        </transition>
        <event type="task-create">
            <action expression="#{taskInstanceUtil.setFrameDefaultTransition('(Cejusc) Resdistribuido')}"/>
            <action expression="#{tramitacaoProcessualService.gravaVariavel('pje:fbc:EhDoCejusc','true')}"/>
        </event>
        <event type="task-end">
            <action expression="#{preencherMovimento.deCodigo(849).lancarMovimento()}"/>
        </event>
    </task-node>
    <process-state name="(Cejusc) Resdistribuido">
        <sub-process name="Redistribuíção" binding="late"/>
        <transition to="Término" name="Término"/>
    </process-state>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - (Cejusc) Arquivamento">
        <task name="Nó de Desvio - (Cejusc) Arquivamento" swimlane="Nó de Desvio - (Cejusc) Arquivamento"/>
        <transition to="Término" name="Término"/>
        <transition to="(Cejusc) Arquivo definitivo" name="(Cejusc) Arquivo definitivo"/>
        <transition to="(Cejusc) Redistribuir" name="(Cejusc) Distribuir"/>
    </task-node>  
    <!-- PROCESS-EVENTS -->
    <event type="node-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="superstate-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="node-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="before-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="superstate-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="transition">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-created">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="after-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-assign">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-create">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="timer">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event> 
</process-definition>
