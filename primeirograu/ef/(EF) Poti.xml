    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - (EF) Poti">
        <task name="Nó de Desvio - (EF) Poti" swimlane="Nó de Desvio - (EF) Poti"/>
        <transition to="(EF) Poti - Em processamento" name="(EF) Poti - Em processamento"/>
        <transition to="(EF) Poti - Bloqueio integral" name="(EF) Poti - Bloqueio integral"/>
        <transition to="(EF) Poti - Bloqueio parcial" name="(EF) Poti - Bloqueio parcial"/>
        <transition to="(EF) Poti - Bloqueio não efetivado (Insuficiência de saldo)" name="(EF) Poti - Bloqueio não efetivado (Insuficiência de saldo)"/>
        <transition to="(EF) Poti - Erros" name="(EF) Poti - Erros"/>
        <transition to="Término" name="Término"/>
        <transition to="(EF) Poti" name="(EF) Poti"/>
