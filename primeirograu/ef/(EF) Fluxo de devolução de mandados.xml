<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="(EF) Fluxo de devolução de mandados">
    <description><![CDATA[Código do fluxo: EF_DEV_MANDADO

Fluxo de devolução de mandados.

Parâmetro: pje:fluxo:incidental:sempreDisparar]]></description>  
    <!-- SWIMLANES -->
    <swimlane name="Nó de Desvio - (EF) Fluxo de devolução de mandados">
        <assignment actor-id="#{actor.id}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial"/>
        <transition to="(EF) É execução fiscal?" name="(EF) É execução fiscal?"/>
    </start-state>  
    <!-- NODES -->
    <decision expression="#{parametroUtil.getParametro('pje:tjrn:classesVEF').contains('_'.concat(tramitacaoProcessualService.recuperaProcesso().classeJudicial.codClasseJudicial).concat('_')) ? '(EF) Lançar sinalizador no fluxo' : 'Término'}" name="(EF) É execução fiscal?">
        <transition to="Término" name="Término"/>
        <transition to="(EF) Atualizar frameDefaultTransition" name="(EF) Atualizar frameDefaultTransition"/>
    </decision>
    <node name="(EF) Atualizar frameDefaultTransition">
        <transition to="(EF) Lançar sinalizador no fluxo" name="(EF) Lançar sinalizador no fluxo"/>
        <event type="node-enter">
            <action expression="#{entityManager.createNativeQuery(parametroUtil.getParametro('pje:update_devolucao_mandado')).setParameter('idProcesso1', tramitacaoProcessualService.recuperaVariavel('processo')).setParameter('idProcesso2', tramitacaoProcessualService.recuperaVariavel('processo')).executeUpdate()}"/>
        </event>
    </node>
    <node name="(EF) Lançar sinalizador no fluxo">
        <transition to="Término" name="Término"/>
        <event type="node-enter">
            <action expression="#{processoJudicialService.sinalizarFluxo(tramitacaoProcessualService.recuperaProcesso(), 'pje:aguarda_devolucao_mandado', true, false, true)};"/>
        </event>
    </node>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - (EF) Fluxo de devolução de mandados">
        <task name="Nó de Desvio - (EF) Fluxo de devolução de mandados" swimlane="Nó de Desvio - (EF) Fluxo de devolução de mandados"/>
        <transition to="Término" name="Término"/>
    </task-node>  
    <!-- PROCESS-EVENTS -->
    <event type="superstate-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="before-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-created">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-create">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="transition">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-assign">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="after-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="timer">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="node-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="superstate-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="node-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event> 
</process-definition>
