<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="(EF) Controle de retorno dos Correios"> 
    <!-- SWIMLANES -->
    <swimlane name="Nó de Desvio - (EF) Controle de retorno dos Correios">
        <assignment actor-id="#{actor.id}"/>
    </swimlane>
    <swimlane name="Secretaria">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('5:1338,5:5483,6:1469,8:5852,8:5853,6:5901,7:5197')}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial"/>
        <transition to="(EF) Citação aguardando Correios - ANALISAR" name="(EF) Citação aguardando Correios - ANALISAR"/>
    </start-state>  
    <!-- NODES -->
    <task-node end-tasks="true" name="(EF) Citação aguardando Correios - ANALISAR">
        <task name="(EF) Citação aguardando Correios - ANALISAR" swimlane="Secretaria"/>
        <transition to="Nó de Desvio - (EF) Controle de retorno dos Correios" name="Nó de Desvio - (EF) Controle de retorno dos Correios">
            <condition expression="#{true}"/>
        </transition>
        <transition to="(EF) Citação recebida dos Correios - ANALISAR" name="Citação recebida dos Correios"/>
    </task-node>
    <task-node end-tasks="true" name="(EF) Citação recebida dos Correios - ANALISAR">
        <task name="(EF) Citação recebida dos Correios - ANALISAR" swimlane="Secretaria"/>
        <transition to="Nó de Desvio - (EF) Controle de retorno dos Correios" name="Nó de Desvio - (EF) Controle de retorno dos Correios">
            <condition expression="#{true}"/>
        </transition>
        <transition to="Término" name="Encaminhar para citação com prazo em curso"/>
        <transition to="(EF) Citação aguardando Correios - ANALISAR" name="Retornar para Citação aguardando Correios"/>
    </task-node>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - (EF) Controle de retorno dos Correios">
        <task name="Nó de Desvio - (EF) Controle de retorno dos Correios" swimlane="Nó de Desvio - (EF) Controle de retorno dos Correios"/>
        <transition to="Término" name="Término"/>
        <transition to="(EF) Citação recebida dos Correios - ANALISAR" name="(EF) Citação recebida dos Correios - ANALISAR"/>
        <transition to="(EF) Citação aguardando Correios - ANALISAR" name="(EF) Citação aguardando Correios - ANALISAR"/>
    </task-node>  
    <!-- PROCESS-EVENTS -->
    <event type="superstate-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="before-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-created">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-create">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="transition">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-assign">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="after-signal">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="timer">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="task-start">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="subprocess-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="node-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="process-end">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="superstate-leave">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="node-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event> 
</process-definition>
