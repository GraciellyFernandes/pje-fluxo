<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="(EF) Encerramento de prazo">
    <description><![CDATA[Código do fluxo: EF_ENC_PRAZO.

Descrição: Fluxo responsável pelo encerramento de prazos.

Raia        Localização   Papel
Secretaria  Secretaria    Chefe de Secretaria
Secretaria  Gabinete      Magistrado]]></description>  
    <!-- SWIMLANES -->
    <swimlane name="Nó de Desvio - (EF) Encerramento de prazo">
        <assignment actor-id="#{actor.id}"/>
    </swimlane>
    <swimlane name="Secretaria">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('5:1338,6:1469')}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial"/>
        <transition to="(EF) Processos pendentes de encerramento de prazo - ENCERRAR" name="(EF) Processos pendentes de encerramento de prazo - ENCERRAR"/>
    </start-state>  
    <!-- NODES -->
    <task-node end-tasks="true" name="(EF) Processos pendentes de encerramento de prazo - ENCERRAR">
        <task name="(EF) Processos pendentes de encerramento de prazo - ENCERRAR" swimlane="Secretaria">
            <controller>
                <variable name="WEB-INF_xhtml_flx_exped_encerrarExpedienteManual" mapped-name="frame:WEB-INF_xhtml_flx_exped_encerrarExpedienteManual" access="read,write"/>
            </controller>
        </task>
        <description><![CDATA[* Variáveis *

1. Variável: WEB-INF_xhtml_flx_exped_encerrarExpedienteManual
   Label: Encerramento de prazo
   Escrita: Sim
   Obrig.: Não
   Tipo: Frame]]></description>
        <transition to="Nó de Desvio - (EF) Encerramento de prazo" name="Nó de Desvio - (EF) Encerramento de prazo">
            <condition expression="#{true}"/>
        </transition>
        <transition to="(EF) Devolução à secretaria" name="Cancelar e sair da tarefa"/>
    </task-node>
    <process-state name="(EF) Devolução à secretaria">
        <sub-process name="(EF) Fluxo de devolução à secretaria" binding="late"/>
        <transition to="Término" name="Término"/>
    </process-state>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - (EF) Encerramento de prazo">
        <task name="Nó de Desvio - (EF) Encerramento de prazo" swimlane="Nó de Desvio - (EF) Encerramento de prazo"/>
        <transition to="Término" name="Término"/>
        <transition to="(EF) Processos pendentes de encerramento de prazo - ENCERRAR" name="(EF) Processos pendentes de encerramento de prazo - ENCERRAR"/>
    </task-node>  
    <!-- PROCESS-EVENTS -->
    <event type="node-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
    <event type="superstate-leave">
